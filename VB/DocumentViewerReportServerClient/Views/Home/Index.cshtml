@model List<DocumentViewerReportServerClient.Models.DocumentItem>

@if (Model.Count > 0) {
    <h3>Report Server Reports List:</h3>
    <ul>
        @foreach (var item in Model) {
            <li>
                <a href="@Url.Action("Viewer", "Home", item, null)" target="_blank">@item.name</a>
            </li>
        }
    </ul>
}
else {
    <h3>No Reports were found on the Report Server</h3>
}